class Employee:
    def __init__(self, name, salary):
        self.set_name(name)
        self.set_salary(salary)

    def get_name(self):
        return self.name

    def set_name(self, name):
        self.name = name

    def get_salary(self):
        return self.salary

    def set_salary(self, salary):
        self.salary = salary

    def display(self):
        print(f"{self.name}, {self.salary}")