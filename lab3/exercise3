num_lec = int(input("Enter a number of lectures:"))
gpa = float(input("Enter your GPA:"))

if gpa >= 2.0:
  if num_lec < 47:
    print("Not enough number of lectures!")
  else:
    print("GRADUATED!!!")
else:
  if num_lec < 47:
    print("Not enough number of lectures and GPA!")
  else:
    print("Not enough GPA!")
