a = int(input("Enter A for equation (ax2 + bx + c)"))
b = int(input("Enter B for equation (ax2 + bx + c)"))
c = int(input("Enter C for equation (ax2 + bx + c)"))

delta = b ** 2 - 4 * a * c
root1 = (-b - delta ** 0.5) / 2 * a
root2 = (-b + delta ** 0.5) / 2 * a

if delta > 0:
  print("There are two real roots")
  print("Roots: " , root1 , "," , root2)
elif delta == 0:
  print("There is on real root.")
  print(root1)
else:
  print("There are two complex roots")
