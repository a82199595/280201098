how_many = int(input("How many Fibonacci numbers:"))
fibonacci_list = []
fibonacci_list.append(1)
fibonacci_list.append(1)

index = 2
while True:
    fibonacci_list.append(fibonacci_list[index - 2] + fibonacci_list[index - 1])
    index += 1
    if how_many == index:
        break

print(fibonacci_list)
